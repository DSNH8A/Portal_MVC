            <summary>
            Creates an Employee.
            </summary>
            <remarks>
            Sample request:
            
                POST api/Employee
                {        
                  "firstName": "Mike",
                  "lastName": "Andrew",
                  "emailId": "Mike.Andrew@gmail.com"        
                }
            </remarks>
            <param name="employee"></param>  
            <response code="201">Returns the newly created item</response>
            <response code="400">If the item is null</response>      
        </member>
